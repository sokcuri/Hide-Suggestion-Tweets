{
    "name": "Hide Suggestion Tweets",
    "description": "Hide suggestion tweet on twitter.com and mobile.twitter.com",
    "version": "1.0",
    "homepage_url": "https://github.com/sokcuri/Hide-Suggestion-Tweet",
    "manifest_version": 2,
    "content_scripts": [{
        "js": ["contentscript.js"],
        "matches": ["*://mobile.twitter.com/*","*://twitter.com/*"]
    }],
    "web_accessible_resources": ["script.js"]
}